<h1>New unlike</h1>

<% form_for(@unlike) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :keyword %><br />
    <%= f.text_field :keyword %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', unlikes_path %>